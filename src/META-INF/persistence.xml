<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="SkyDentalSysPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>app.model.MedicalDentalHistory</class>
    <class>app.model.Gender</class>
    <class>app.model.CivilStatus</class>
    <class>app.model.Patient</class>
    <class>app.model.PatientMdh</class>
    <class>app.model.MdhCategory</class>
    <class>app.model.ToothStatus</class>
    <class>app.model.controller.PatientToothStatus</class>
    <class>app.model.PatientToothStatus</class>
    <class>app.model.PatientToothPartStatus</class>
    <class>app.model.Dentist</class>
    <class>app.model.Branch</class>
    <class>app.model.Clinic</class>
    <class>app.model.Payment</class>
    <class>app.model.TreatmentRecord</class>
    <class>app.model.PatientAttachment</class>
    <class>app.model.TreatmentRecordLog</class>
    <class>app.model.InformedConsent</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3307/dental_db?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="sduser"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="sduser"/>
    </properties>
  </persistence-unit>
</persistence>
